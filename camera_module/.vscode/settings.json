{
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.formatting.provider": "none",
  "cmake.configureOnOpen": false,
  "files.associations": {
    "*.vue": "vue",
    "*.tcc": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "cstddef": "cpp",
    "cstdio": "cpp",
    "cwchar": "cpp",
    "vector": "cpp",
    "functional": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "optional": "cpp",
    "string_view": "cpp",
    "type_traits": "cpp",
    "random": "cpp",
    "fstream": "cpp",
    "new": "cpp",
    "stdexcept": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "typeinfo": "cpp",
    "utility": "cpp",
    "variant": "cpp"
  }
}
